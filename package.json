{
  "name": "node-red-contrib-chatbot",
  "version": "0.8.0-beta-2",
  "description": "REDBot a Chat bot for a full featured chat bot for Telegram**, Facebook Messenger and Slack. Almost no coding skills required",
  "scripts": {
    "test": "mocha",
    "ngrok": "ngrok http 127.0.0.1:1880",
    "localtunnel": "lt --port 1880",
    "lint": "eslint *.js ./lib/**.js",
    "pull-wiki": "(cd ./wiki && git pull)",
    "commit-wiki": "git add . && git commit -m \"docs\"",
    "build": "npm run pull-wiki && node ./bin/generate-docs",
    "version": "npm run pull-wiki && node ./bin/generate-docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guidone/node-red-contrib-chatbot.git"
  },
  "node-red": {
    "nodes": {
      "chatbot-message": "chatbot-message.js",
      "chatbot-analytics": "chatbot-analytics.js",
      "chatbot-context": "chatbot-context.js",
      "chatbot-topic": "chatbot-topic.js",
      "chatbot-apiai": "chatbot-apiai.js",
      "chatbot-voice": "chatbot-voice.js",
      "chatbot-language": "chatbot-language.js",
      "chatbot-listen": "chatbot-listen.js",
      "chatbot-parse": "chatbot-parse.js",
      "chatbot-command": "chatbot-command.js",
      "chatbot-document": "chatbot-document.js",
      "chatbot-video": "chatbot-video.js",
      "chatbot-image": "chatbot-image.js",
      "chatbot-audio": "chatbot-audio.js",
      "chatbot-location": "chatbot-location.js",
      "chatbot-ask": "chatbot-ask.js",
      "chatbot-inline-buttons": "chatbot-inline-buttons.js",
      "chatbot-quick-replies": "chatbot-quick-replies.js",
      "chatbot-generic-template": "chatbot-generic-template.js",
      "chatbot-list-template": "chatbot-list-template.js",
      "chatbot-request": "chatbot-request.js",
      "chatbot-log": "chatbot-log.js",
      "chatbot-qrcode": "chatbot-qrcode.js",
      "chatbot-conversation": "chatbot-conversation.js",
      "chatbot-debug": "chatbot-debug.js",
      "chatbot-waiting": "chatbot-waiting.js",
      "chatbot-authorized": "chatbot-authorized.js",
      "chatbot-transport": "chatbot-transport.js",
      "chatbot-rivescript": "chatbot-rivescript.js",
      "chatbot-telegram-receive": "chatbot-telegram-receive.js",
      "chatbot-slack-receive": "chatbot-slack-receive.js",
      "chatbot-messenger-menu": "chatbot-messenger-menu.js",
      "chatbot-facebook-receive": "chatbot-facebook-receive.js",
      "chatbot-smooch-receive": "chatbot-smooch-receive.js"
    }
  },
  "maintainers": [
    {
      "name": "Guido Bellomo",
      "email": "guido.bellomo@gmail.com",
      "web": "http://javascript-jedi.com"
    }
  ],
  "keywords": [
    "node-red"
  ],
  "author": "Guido Bellomo <guido.bellomo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@slack/client": "^3.5.1",
    "apiai": "^3.0.0",
    "cli-color": "^1.1.0",
    "compromise": "^7.0.28",
    "cookie-parser": "^1.4.3",
    "fast-levenshtein": "^1.1.3",
    "jszip": "^3.1.3",
    "languagedetect": "^1.1.1",
    "marked": "^0.3.6",
    "mime": "^1.3.4",
    "moment": "^2.13.0",
    "nlp_compromise": "^6.5.3",
    "node-emoji": "^1.3.1",
    "node-telegram-bot-api": "^0.27.1",
    "prettyjson": "^1.2.1",
    "qr-image": "^3.1.0",
